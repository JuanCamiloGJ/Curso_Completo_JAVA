public class PrimeraClase {
	
	public static void main(String args[]) {
		System.out.println("Hola papu ¿estas aquí nuevamente?");
	
		System.out.println("estamos haciendo pruebas de git y github");
		
		Var prueba=new Var();
		prueba.PruebaVar();
			
	}
	
	
	
}
class Var {
	
	private int git=3;
	private String frase="Este es el vídeo:";
	
	public void PruebaVar() {
		System.out.println(frase+ " "+ git+"del curso de git y git hub");
	
		System.out.println("Lo mejor del mundo mundial lo de git");
	}
}
